
#include <iostream>

int main()
{
    int length =10 ;
    std::cout<<"Hello World \n";
    int i , j , temp;
    int a[length/2] , b[length/2] , c[6];
    
    for(int i=0 ; i<length/2; i++){
        if(i<length/2){
         std::cin>>a[i] ;
        }
        //  if(i>=length/2){
        //       std::cout<< ">+ \n" ;
        //      std::cin>>b[i-length/2];
        //  }
         
    }
    std::cout<< "done \n" ;
    
    for(i=0; i< length/2 ; i++){
        for(int j=0 ; j< length/2 -1-i ; j++){
            int temp;
            if(i< length/2 && j< length/2 ){
                
               if ( a[j]>a[j+1]){
                temp =a[j+1] ;
                a[j+1]= a[j] ;
                a[j]= temp ;
               }
                
            }
            }
        }
        
        
    
    
    
    
    
    
    
    
    
    for( i=0 ; i<length/2; i++){
        if(i<length/2){ std::cout<<a[i] <<"\n" ;}
        //  if(i>=length/2){
        //      std::cout<<b[i-length/2]<<"\n";
        //  }
         
    }
    

    return 0;
}
